{
    "@jupyterlab/apputils-extension:themes": {
        "theme": "Qtheme DK",
        "theme-scrollbars": true
    },
    "@jupyterlab/application-extension:sidebar": {
        "jp-running-sessions": "right",
        "command-palette": "left",
        "notebook-tools": "right",
        "tab-manager": "left",
        "table-of-contents": "left",
        "dask-dashboard-launcher": "right",
        "filebrowser": "right",
        "jp-property-inspector": "right",
        "extensionmanager.main-view": "right"
    },
    "@jupyterlab/statusbar-extension:plugin": {
        "visible": false
    },
    "jupyterlab/terminal-extension:plugin": {
        "theme": "light"
    },
    "@deathbeds/jupyterlab-fonts:fonts": {
        "styles": {
            ":root": {
                "--jp-code-font-family": "'Fira Code Medium'"
            }
        }
    },
    "@jupyterlab/filebrowser-extension:browser":{
        "navigateToCurrentDirectory": true   
    }
  }